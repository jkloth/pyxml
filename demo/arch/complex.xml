<?xml version="1.0"?>
<?test where="before"?>
<?IS10744:arch name ="biblio" renamer-att="bren" suppressor-att="bsup" ignore-data-att="bign"
               bridge-form="bibbrid"?>
<?IS10744:arch name ="html" suppressor-att="hsup" auto="ArcAuto" form-att="ht"?>
<?test where="after"?>

<!DOCTYPE doc [
<!ATTLIST content ht     CDATA  #FIXED   "body"     >
